run main;

proc main() do
    let one = 1;
    let six = 2 * 3;
    let number = one + six;

    let val = 3.14;
    action print: @str(val);

    if number == 7 then
        action print: "number is 7";
    else
        action print: "number is not 7";
    end

    for x in one..number do
        action print: @str(x);
    end

    do
        action wait: 1.0;

        action waitframes: 30;
    end

    if 2 + 2 == 5 then
        action print: @str(4);
    else
        if true then
            action print: "true";
        else
            action print: "false";
        end
    end
end
