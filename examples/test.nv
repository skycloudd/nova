run main;

proc main() do
    let one = 1;
    let six = 2 * 3;
    let number = one + six;

    action print: number == 7;

    for x in one..number do
        action print: x;
    end

    do
        action wait: 1.0;

        action waitframes: 30;
    end

    if 2 + 2 == 5 then
        action print: 4;
    else
        action print: true;
    end
end
