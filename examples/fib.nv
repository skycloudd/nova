func main(): int do
    for n in 0..10 do
        fib_recursive(n);
        fib_iterative(n);
    end

    return 0;
end

func fib_recursive(n: int): int do
    if n <= 1 then
        return n;
    end

    return fib_recursive(n - 1) + fib_recursive(n - 2);
end

func fib_iterative(n: int): int do
    let a = 0;
    let b = 1;

    for i in 0..n do
        let c = a + b;
        a = b;
        b = c;
    end

    return a;
end
