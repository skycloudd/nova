{
    "name": "sowo-language",
    "displayName": "Sowo language",
    "description": "Language support for the Sowo programming language",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.85.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "sowo",
                "aliases": [
                    "Sowo",
                    "sowo"
                ],
                "extensions": [
                    ".sw",
                    ".sowo"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "sowo",
                "scopeName": "source.sowo",
                "path": "./syntaxes/sowo.tmLanguage.json"
            }
        ]
    }
}
